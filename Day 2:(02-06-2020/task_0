//1.Add height and weight to Fluffy
var cat = {
 name: "Fluffy",
  weight: 3,
   height: 3,
 activities: ["play", "eat cat food"],
 catFriends: [
 {
 name: "bar",
 activities: ["be_grumpy", "eat bread omblet"],
 weight: 8,
 furcolor: "white"
 }, 
 {
 name: "foo",
 activities: ["sleep", "pre sleep naps"],
 weight: 3
 }
 ]
}
console.log(cat);
//2.Fluffy name is spelled wrongly. Update it to Fluffyy
var cat = {
 name: "Fluffy",
  weight: 3,
   height: 3,
 activities: ["play", "eat cat food"],
 catFriends: [
 {
 name: "bar",
 activities: ["be_grumpy", "eat bread omblet"],
 weight: 8,
 furcolor: "white"
 }, 
 {
 name: "foo",
 activities: ["sleep", "pre sleep naps"],
 weight: 3
 }
 ]}
 cat.name="Fulffyy";
console.log(cat);
