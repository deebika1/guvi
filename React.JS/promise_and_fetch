<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <script>
      fetch("https://restcountries.eu/rest/v2/all")
        .then((data) => {
          let a = data.json();
          console.log("success:::", a);
          return a;
        })
        .then((data) => {
          console.log("This is the parsed json1", data);
          return fetch("https://restcountries.eu/rest/v2/name/united");
        })
        .then((data) => {
          let a = data.json();
          console.log("2 nd sucess:::", a);
          return a;
        })
        .then((data) => {
          console.log("This is the parsed json2", data);
        })
        .catch((error) => {
          console.log("error :(");
        });
    </script>
  </head>
  <body></body>
</html>
